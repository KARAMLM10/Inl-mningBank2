@page
@model InlämningBank2.Pages.Account.WithdrawModel
@{
}

<h1>Withdraw</h1>
<div class="col-md-3">
    <button type="submit" class="btn btn-primary btn-block" onclick="window.history.back()">Back</button>
</div>
<h3>Balance: @Model.Balance kr</h3>
<form method="post">
    <div class="form-group">
        <label asp-for="Amount">Amount</label>
        <input class="form-control" asp-for="Amount" />
        <span class="field-validation-error" asp-validation-for="Amount"></span>
    </div>
    <button type="submit" asp-route-customerId="@Model.CustomerId" asp-route-accountId="@Model.AccountId" class="btn btn-primary">Withdraw</button>
</form>

